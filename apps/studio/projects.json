{
    "root": "apps/studio",
    "targets": {
      "build": {
        "executor": "nx:run-commands",
        "options": {
          "cwd": "apps/studio",
          "command": "dotenv -- sanity build"
        }
      },
      "deploy": {
        "executor": "nx:run-commands",
        "options": {
          "cwd": "apps/studio",
          "command":"dotenv -- npx sanity deploy"
        }
      },
      "undeploy": {
        "executor": "nx:run-commands",
        "options": {
          "cwd": "apps/studio",
          "command":"dotenv -- npx sanity undeploy"
        }
      },
      "dev": {
        "executor": "nx:run-commands",
        "options": {
          "cwd": "apps/studio",
          "command":"dotenv -e ../../.env.development -- sanity dev"
        }
      }
    }
  }   
  